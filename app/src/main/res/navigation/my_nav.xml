<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/main_page">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.evcharger.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_MNVFragment"
            app:destination="@id/MNVFragment" />
        <action
            android:id="@+id/action_loginFragment_to_main_page"
            app:destination="@id/main_page" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.evcharger.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/MNVFragment"
        android:name="com.example.evcharger.MNVFragment"
        android:label="fragment_m_n_v"
        tools:layout="@layout/fragment_m_n_v" >
        <action
            android:id="@+id/action_MNVFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_MNVFragment_to_setFragment"
            app:destination="@id/setFragment" />
        <action
            android:id="@+id/action_MNVFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
    </fragment>
    <fragment
        android:id="@+id/setFragment"
        android:name="com.example.evcharger.SetFragment"
        android:label="fragment_set"
        tools:layout="@layout/fragment_set" >
        <action
            android:id="@+id/action_setFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_setFragment_to_customerService_page"
            app:destination="@id/customerService_page"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.evcharger.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/main_page"
        android:name="com.example.evcharger.main_page"
        android:label="fragment_main_page"
        tools:layout="@layout/fragment_main_page" >
        <action
            android:id="@+id/action_main_page_to_setFragment"
            app:destination="@id/setFragment"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_main_page_to_blankFragment_test"
            app:destination="@id/blankFragment_test" />
        <action
            android:id="@+id/action_main_page_to_payFragment"
            app:destination="@id/payFragment"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment_test"
        android:name="com.example.evcharger.BlankFragment_test"
        android:label="fragment_blank_test"
        tools:layout="@layout/fragment_blank_test" />
    <activity
        android:id="@+id/customerService_page"
        android:name="com.example.evcharger.CustomerService_page"
        android:label="CustomerService_page" />
    <fragment
        android:id="@+id/judge_page"
        android:name="com.example.evcharger.judge_page"
        android:label="fragment_judge_page"
        tools:layout="@layout/fragment_judge_page" />
    <fragment
        android:id="@+id/payFragment"
        android:name="com.example.evcharger.PayFragment"
        android:label="PayFragment" >
        <action
            android:id="@+id/action_payFragment_to_judge_page"
            app:destination="@id/judge_page"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

</navigation>